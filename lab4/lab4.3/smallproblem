(defproblem problem domain
;;Initial state
( ;;at, has, etc. 
    (uav uav1)
    (carrier carrier1)
    (location depot)
    (location loc1)
    (location loc2)
    (contents medicine)
    (contents food)

    (free uav1)
    (at uav1 depot)
    (at carrier1 depot)
    (capacity carrier1 4)
    (load carrier1 0)

    (has depot food 20)
    (has depot medicine 23)

    (needs loc1 medicine 8)
    (has loc1 medicine 0)
    (needs loc2 food 1)
    (has loc2 food 0)
)
;; Initial task list
(
    (
        (deliver-all)
    )
)
)