(defproblem problem domain
;;Initial state
( ;;at, has, etc. 
    (uav uav1)
    (carrier carrier1)
    (location depot)
    (location loc1)
    (location loc2)
    (contents medicine)
    (contents food)

    (free uav1)
    (at uav1 depot)
    (at carrier1 depot)
    (capacity carrier1 4)
    (load carrier1 0)

    (has depot food 4)
    (has depot medicine 8)

    (needs loc1 medicine 2)
    (has loc1 medicine 0)
    (needs loc1 food 2)
    (has loc1 food 0)
    (needs loc2 food 2)
    (has loc2 food 0)
)
;; Initial task list
(
    (
        (deliver-all)
    )
)
)